<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seqdb_barcode_scanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
          
        
        <activity
            android:name="com.example.seqdb_barcode_scanner.MainActivity"
            android:label="@string/app_name" >

        </activity>
        
        <activity
            android:name="com.example.seqdb_barcode_scanner.LoginActivity"
            android:label="Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
         <activity
            android:name="com.example.seqdb_barcode_scanner.MainMenuActivity"
            android:label="Main Menu" >
        </activity>
        
         <activity
            android:name="com.example.seqdb_barcode_scanner.ScanPlaceholderActivity"
            android:label="Barcode Scanner">
         </activity>
        
         <activity
            android:name="com.example.seqdb_barcode_scanner.LookupActivity"
            android:label="Lookup" >
        </activity>

        
  
    </application>

</manifest>
